public class CheckArrayForN{

		public static void main(String[] args) {
			
			int arr[] ={1,2,3,0,4,5};
			for(int i=0;i<arr.length-1;i++)
			{
				for(int j=0;j<arr.length;j++)
				{
					if(arr[i]+arr[j]==5)
					{
						System.out.println("[i]    "+i + "		j "+j + " 	i+j-> " + (i+j));
					}
				}
				
			}

			// 04  - 12 - 35
		}
	}
